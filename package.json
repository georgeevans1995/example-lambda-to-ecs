    "ts-jest": "^27.1.4",
    "ts-node": "^10.8.1",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-cdk-lib": "2.23.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1167.0",
